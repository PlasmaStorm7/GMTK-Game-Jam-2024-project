[editor_plugins]

enabled=PackedStringArray("res://addons/gdfxr/plugin.cfg", "res://addons/imrp/plugin.cfg", "res://addons/maaacks_game_template/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

